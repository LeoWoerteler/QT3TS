   <!--* error conditions *-->

   <test-case name="parse-ietf-date-errs-0">
      <description>Error FORG0010 syntax of date is incorrect</description>
      <created by="Liam Quin" on="2014-08-22"/>
      <test>parse-ietf-date("2014-08-20T19:36:01Z")</test>
      <result>
         <error code="FORG0010"/>
